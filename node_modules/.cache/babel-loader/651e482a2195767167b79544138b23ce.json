\n        this.$refs.bigChart.updateGradients(chartData);\r\n        this.bigLineChart.chartData = chartData;\r\n        this.bigLineChart.activeIndex = index;\r\n      }\r\n    },\r\n    mounted() {\r\n      this.i18n = this.$i18n;\r\n      if (this.enableRTL) {\r\n        this.i18n.locale = 'ar';\r\n        this.$rtl.enableRTL();\r\n      }\r\n      this.initBigChart(0);\r\n    },\r\n    beforeDestroy() {\r\n      if (this.$rtl.isRTL) {\r\n        this.i18n.locale = 'en';\r\n        this.$rtl.disableRTL();\r\n      }\r\n    }\r\n  };\r\n</script>\r\n<style>\r\n</style>\r\n"],"sourceRoot":"src/pages"}]}